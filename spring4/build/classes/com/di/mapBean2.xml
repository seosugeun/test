<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean id="propsObject" class="com.di.PropertiesObject">
		<property name="someProps">
			<props>
				<prop key="/di/testList.sp4">testList</prop>
				<prop key="/di/testAdd.sp4">testAdd</prop>
				<prop key="/di/testUpd.sp4">testUpd</prop>
				<prop key="/di/testDel.sp4">testDel</prop>
			</props>
		</property>
	</bean>

	<bean id="moreComplexObject" class="com.di.ComplexObject">
		<property name="someMap">
			<map>
				<entry>
					<key><value>first</value></key>
					<value>석명고</value>
				</entry>
				<entry>
					<key><value>second</value></key>
					<value>현광이</value>
				</entry>
				<entry>
					<key><value>third</value></key>
					<value>호준손</value>
				</entry>
			</map>
		</property>
	</bean>
</beans>